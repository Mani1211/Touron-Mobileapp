{
  "expo": {
    "scheme": "https",
    "name": "tour On",
    "slug": "touron",
    "version": "1.3.4",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "updates": {
      "enabled": true
    },
    "ios": {
      "splash": {
        "resizeMode": "cover",
        "image": "./assets/Splash Screen/ios/splash.png"
      },
      "associatedDomains": ["applinks:com.touron.travelapp"],
      "bundleIdentifier": "com.touron.travelapp",
      "supportsTablet": false,
      "buildNumber": "1.3.4",
      "usesIcloudStorage": false,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos to upload profile pictures.",
        "NSPhotoLibraryAddUsageDescription": "This app uses photo bibrary to pick and save photos",
        "NSPhotoLibraryUsageDescription": "This app uses photo bibrary to pick and save photos"
      }
    },
    "android": {
      "splash": {
        "resizeMode": "cover",
        "ldpi": "./assets/Splash Screen/android/drawable-ldpi/splash.png",
        "mdpi": "./assets/Splash Screen/android/drawable-mdpi/splash.png",
        "hdpi": "./assets/Splash Screen/android/drawable-hdpi/splash.png",
        "xhdpi": "./assets/Splash Screen/android/drawable-xhdpi/splash.png",
        "xxhdpi": "./assets/Splash Screen/android/drawable-xxhdpi/splash.png",
        "xxxhdpi": "./assets/Splash Screen/android/drawable-xxxhdpi/splash.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.touron.in"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "enableDangerousExperimentalLeanBuilds": true,
      "package": "com.touron.travelapp",
      "versionCode": 32,
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "assetBundlePatterns": ["./assets/**/*"],
    "description": "",
    "androidStatusBar": {
      "backgroundColor": "#FFF"
    }
  }
}
